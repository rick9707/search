	"license": "MIT",
	"scripts": {
		"build": "node build.js",
		"test": "ts-node --project ./tsconfig.json -r tsconfig-paths/register test/index.ts",
		"eslint": "eslint '**/*/*.{ts}'",
		"local": "NODE_ENV=local NODE_PATH=. nodemon dist/src/server.js --env=local",
		"dev": "NODE_ENV=dev NODE_PATH=. nodemon dist/src/server.js --env=dev",
		"start": "node --trace-deprecation -r module-alias/register dist/src/server.js",
		"start:prod": "NODE_ENV=prod node --trace-deprecation -r module-alias/register dist/src/server.js --env=prod"
	},
	"_moduleAliases": {
		"@root": "dist/src",
		"@controllers": "dist/src/controllers",
		"@utils": "dist/src/utils",
		"@routes": "dist/src/routes",
		"@instances": "dist/src/instances",
		"@models": "dist/src/models",
		"@services": "dist/src/services",
		"@rules": "dist/src/rules",
		"@repositories": "dist/src/repositories",
		"@middlewares": "dist/src/middlewares",
		"@dto": "dist/src/dto"
	},
